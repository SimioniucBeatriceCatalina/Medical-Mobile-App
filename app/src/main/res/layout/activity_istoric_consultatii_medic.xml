<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IstoricConsultatiiMedic"
    android:background="#e3e7eb">

    <com.vivekkaushik.datepicker.DatePickerTimeline
        android:id="@+id/datePickerTimeline"
        android:layout_width="427dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="107dp"
        android:layout_marginEnd="20dp"
        app:dateTextColor="#3c7997"
        app:dayTextColor="#3c7997"
        app:layout_constraintBottom_toTopOf="@+id/listamedic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:monthTextColor="#3c7997" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="137dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="43dp"
        android:text="Consultatii"
        android:textColor="#3c7997"
        android:textSize="20dp"
        android:textStyle="bold"
        app:fontFamily="@font/cutive"
        app:layout_constraintBottom_toTopOf="@+id/datePickerTimeline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listamedic"
        android:layout_width="407dp"
        android:layout_height="529dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/datePickerTimeline" />

</androidx.constraintlayout.widget.ConstraintLayout>